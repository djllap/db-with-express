{
  "name": "express-boilerplate",
  "version": "1.0.0",
  "description": "Basic reusable starting files for Express Projects",
  "main": "index.js",
  "scripts": {
    "test": "mocha --require test/setup.js",
    "dev": "nodemon src/server.js",
    "migrate": "postgrator --config postgrator-config.js",
    "migrate:test": "env NODE_ENV=test npm run migrate",
    "start": "node src/server.js",
    "predeploy": "npm audit",
    "deploy": "git push heroku master"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djllap/express-boilerplate.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/djllap/express-boilerplate/issues"
  },
  "homepage": "https://github.com/djllap/express-boilerplate#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "helmet": "^3.21.2",
    "is-url": "^1.2.4",
    "knex": "^0.20.4",
    "morgan": "^1.9.1",
    "pg": "^7.14.0",
    "uuid": "^3.3.3",
    "valid-url": "^1.0.9",
    "winston": "^3.2.1",
    "xss": "^1.0.6"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^6.2.2",
    "nodemon": "^2.0.1",
    "postgrator-cli": "^3.2.0",
    "supertest": "^4.0.2"
  }
}
